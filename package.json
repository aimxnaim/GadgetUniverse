{
  "name": "gadgetuniverse",
  "version": "1.0.0",
  "description": "Fully functional e-Commerce websites using MERN stack",
  "main": "app.js",
  "scripts": {
    "start": "node BackEnd/app.js",
    "dev": "SET NODE_ENV=DEVELOPMENT& nodemon BackEnd/app.js",
    "prod": "SET NODE_ENV=PRODUCTION& node BackEnd/app.js",
    "seeder": "SET NODE_ENV=DEVELOPMENT& node BackEnd/seed/seeder.js",
    "seed:admin": "SET NODE_ENV=DEVELOPMENT& node BackEnd/seed/seedAdmin.js",
    "seed:prod": "SET NODE_ENV=PRODUCTION& node BackEnd/seed/seedAdmin.js",
    "seed:fresh": "node BackEnd/seed/updateDataWithUnsplash.js && npm run seeder",
    "deploy": "npm run seed:prod && npm run prod",
    "deploy:fresh": "SET NODE_ENV=PRODUCTION& node BackEnd/seed/seedAdmin.js && node BackEnd/app.js"
  },
  "author": "Aiman Naim",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.2.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.0",
    "nodemailer": "^6.9.13",
    "stripe": "^15.12.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "nodemon": "^3.1.2"
  }
}
